<h1><%= Admin::InterviewQuestion.model_name.human %> <%= link_to t('crud.new'), new_admin_interview_question_path, class: 'btn btn-lg btn-success' %></h1>

<%= paginate @admin_interview_questions %>

<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <%= th_for Admin::InterviewQuestion, :id, :question, :answer, :sort_id, :is_online %>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_interview_questions.each do |admin_interview_question| %>
      <tr>
        <%= td_for admin_interview_question, :id, :question, :answer, :sort_id, :is_online %>
        <%= td_action_for admin_interview_question %>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @admin_interview_questions %>