<h1><%= Admin::Apply.model_name.human %> <%= link_to t('crud.new'), new_admin_apply_path, class: 'btn btn-lg btn-success' %></h1>

<%= paginate @admin_applies %>

<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <%= th_for Admin::Apply, :id, :course, :stage, :schedule, :first_name, :last_name, :phone, :email, :age, :gender, :message %>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_applies.each do |admin_apply| %>
      <tr>
        <%= td_for admin_apply, :id, [:course, :title], [:stage, :title], [:schedule, :date], :first_name, :last_name, :phone, :email, :age, :gender, :message %>
        <%= td_action_for admin_apply %>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @admin_applies %>