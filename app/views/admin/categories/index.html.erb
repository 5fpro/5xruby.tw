<h1><%= Admin::Category.model_name.human %> <%= link_to t('crud.new'), new_admin_category_path, class: 'btn btn-lg btn-success' %></h1>

<%= paginate @admin_categories %>

<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <%= th_for Admin::Category, :id, :name %>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_categories.each do |admin_category| %>
      <tr>
        <%= td_for admin_category, :id, :name %>
        <%= td_action_for admin_category %>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @admin_categories %>