<h1><%= Admin::Author.model_name.human %> <%= link_to t('crud.new'), new_admin_author_path, class: 'btn btn-lg btn-success' %></h1>

<%= paginate @admin_authors %>

<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <%= th_for Admin::Author, :id, :name %>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_authors.each do |admin_author| %>
      <tr>
        <%= td_for admin_author, :id, :name %>
        <%= td_action_for admin_author %>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @admin_authors %>